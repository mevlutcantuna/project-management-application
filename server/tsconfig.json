{
    "compilerOptions": {
      "target": "ES2022",
      "module": "ESNext",
      "moduleResolution": "node",
      "esModuleInterop": true,
      "allowSyntheticDefaultImports": true,
      "strict": true,
      "outDir": "./dist",
      "rootDir": "./",
      "skipLibCheck": true
    },
    "include":["src/**/*.ts"],
    "exclude": ["node_modules", "dist"]
}